<%- partial('_partial/head') %>
<body>
  <%- partial('_partial/navbar') %>
  <% if (page.sidebar !== false) { %>
    <section class="section">
      <div class="container">
        <div class="columns">
          <div class="column site-sidebar-wrap is-hidden-touch">
            <section class="section pt-0">
              <% if (page.path.startsWith('releases/')) { %>
                <%- partial('_partial/releases-toc', null, { cache: !config.relative_link }) %>
              <% } else { %>
                <%- partial('_partial/sidebar', toc_blocks()) %>
              <% } %>
            </section>
          </div>
          <div class="column site-article-wrap is-three-quarters">
            <section class="section pt-0">
              <%- body %>
            </section>
          </div>
        </div>
      </div>
    </section>
  <% } else if (page.home) { %>
    <%- body %>
  <% } else { %>
    <section class="section">
      <%- body %>
    </section>
  <% } %>
  <%- partial('_partial/footer') %>
</body>
</html>
